#ifndef TERMINAL_H_
#define TERMINAL_H_

#include<sstream>
#include<vector>
#include<algorithm>

std::vector<std::string> parse(std::string input){
	std::istringstream iss(input);
	std::vector<std::string> args;
	do{
		std::string s;
		iss >> s;
		args.push_back(s);
	} while(iss);
	return args;

}

int switchVal(std::string s){
std::transform(s.begin(), s.end(), s.begin(), ::tolower);
if(s == "quit")
}


#endif /* TERMINAL_H_ */
