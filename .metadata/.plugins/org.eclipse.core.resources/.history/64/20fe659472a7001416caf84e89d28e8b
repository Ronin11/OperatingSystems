#ifndef TERMINAL_H_
#define TERMINAL_H_

#include<sstream>

void parse(std::string input){
	std::istringstream iss(input);

	for(int i = 0;; i++){
	    string sub;
	    iss >> sub;
	    cout << "Substring: " << sub << endl;
	 };

}




#endif /* TERMINAL_H_ */
